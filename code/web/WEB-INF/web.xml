    <servlet>
        <servlet-name>o_InsertImageAction</servlet-name>
        <servlet-class>com.biometric.actions.o_InsertImageAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertDataAction</servlet-name>
        <url-pattern>/InsertDataAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>o_InsertDataAction</servlet-name>
        <url-pattern>/o_InsertDataAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OBioIdentiryAction</servlet-name>
